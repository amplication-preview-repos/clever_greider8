datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Bom {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  partId    String?
  productId String?
  quantity  Int?
  updatedAt DateTime @updatedAt
}
